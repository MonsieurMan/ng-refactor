{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": {
            "options": "single"
        },
        "interface-name": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "max-method-function-line-count": [true, 15, "include-nested-functions"]
    },
    "rulesDirectory": [
        "./rules"
    ]
}