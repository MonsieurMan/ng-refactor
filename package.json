{
  "name": "ng-refactor",
  "version": "0.0.6",
  "description": "Essential refactorings for Angular.",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/MonsieurMan/ng-refactor.git"
  },
  "bugs": {
    "email": "ange.g.picard@gmail.com",
    "url": "https://gitlab.com/MonsieurMan/ng-refactor/issues"
  },
  "author": "MrMan",
  "license": "MIT",
  "publisher": "ng-refactor",
  "icon": "assets/ng-refactor.png",
  "main": "./out/extension",
  "readme": "./README.md",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Angular",
    "Refactoring"
  ],
  "activationEvents": [
    "onCommand:ng-refactor.toggleInlineHTML",
    "onCommand:ng-refactor.toggleInlineCSS",
    "onLanguage:typescript"
  ],
  "contributes": {
    "commands": [
      {
        "command": "ng-refactor.toggleInlineHTML",
        "category": "refactor",
        "title": "ng-refactor: Toggle inline HTML"
      },
      {
        "command": "ng-refactor.toggleInlineCSS",
        "category": "refactor",
        "title": "ng-refactor: Toggle inline CSS"
      }
    ]
  },
  "scripts": {
    "start": "tsc -watch -p ./",
    "pretest": "tslint -p tslint.json",
    "vscode:prepublish": "yarn compile && yarn generate-toc",
    "compile": "tsc -p ./",
    "generate-toc": "markdown-toc README.md -i",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^10.5.0",
    "markdown-toc": "^1.2.0",
    "tslint": "^5.10.0",
    "typescript": "^2.1.4",
    "vscode": "^1.1.18"
  }
}
