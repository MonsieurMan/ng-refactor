{
  "name": "ng-refactor",
  "version": "0.0.2",
  "description": "Simple yet useful refactorings for angular.",
  "repository": "https://gitlab.com/MonsieurMan/ng-refactor.git",
  "author": "MrMan",
  "license": "MIT",
  "publisher": "ng-refactor",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ng-refactor.toggleInlineHTML",
    "onLanguage:typescript"
  ],
  "icon": "ng-refactor.png",
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "ng-refactor.toggleInlineHTML",
        "category": "refactor",
        "title": "ng-refactor: Toggle inline HTML"
      }
    ]
  },
  "readme": "./README.md",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^10.5.0",
    "tslint": "^5.10.0",
    "typescript": "^2.1.4",
    "vscode": "^1.1.18"
  }
}
